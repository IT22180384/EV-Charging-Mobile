    <!-- SF Pro Light -->
    <font
        android:font="@font/sf_pro_light"
        android:fontStyle="normal"
        android:fontWeight="300" />
    
    <!-- SF Pro Regular -->
    <font
        android:font="@font/sf_pro_regular"
