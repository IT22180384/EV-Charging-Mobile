<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- SF Pro Display Regular -->
    <font
        android:font="@font/sfprodisplayregular"
        android:fontStyle="normal"
        android:fontWeight="400" />
    
    <!-- SF Pro Display Medium -->
    <font
        android:font="@font/sfprodisplaymedium"
        android:fontStyle="normal"
        android:fontWeight="500" />
    
    <!-- SF Pro Display Bold -->
    <font
        android:font="@font/sfprodisplaybold"
        android:fontStyle="normal"
        android:fontWeight="700" />
    
    <!-- SF Pro Display Light Italic -->
    <font
        android:font="@font/sfprodisplaylightitalic"
        android:fontStyle="italic"
        android:fontWeight="300" />
    
    <!-- SF Pro Display Semibold Italic -->
    <font
        android:font="@font/sfprodisplaysemibolditalic"
        android:fontStyle="italic"
        android:fontWeight="600" />
    
    <!-- SF Pro Display Heavy Italic -->
    <font
        android:font="@font/sfprodisplayheavyitalic"
        android:fontStyle="italic"
        android:fontWeight="800" />
    
    <!-- SF Pro Display Black Italic -->
    <font
        android:font="@font/sfprodisplayblackitalic"
        android:fontStyle="italic"
        android:fontWeight="900" />
    
    <!-- SF Pro Display Thin Italic -->
    <font
        android:font="@font/sfprodisplaythinitalic"
        android:fontStyle="italic"
        android:fontWeight="100" />
    
    <!-- SF Pro Display Ultralight Italic -->
    <font
        android:font="@font/sfprodisplayultralightitalic"
        android:fontStyle="italic"
        android:fontWeight="200" />
</font-family>
